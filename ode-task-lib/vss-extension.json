{
  "manifestVersion": 1,
  "id": "build-release-activies__TargetVersion__",
  "version": "__Build_BuildNumber__",
  "name": "Build & Release Activities",
  "scopes": [ "vso.work", "vso.work_write" ],
  "description": "",
  "baseUri": "https://github.com/hangar18rip/ode-task-lib",
  "publisher": "hangar18",
  "public": false,
  "icons": {
    "default": "img/fb.jpg"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Build",
    "Release"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    },
    "getstarted": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    },
    "learn": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    },
    "support": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    },
    "repository": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    },
    "issues": {
      "uri": "https://github.com/hangar18rip/ode-task-lib"
    }
  },
  "branding": {
    "color": "rgb(53, 53, 53)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "img"
    },
    {
      "path": "tasks"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "contributions": [
    {
      "id": "resolvetoken-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/Resolve-Token"
      }
    },
    {
      "id": "updateversionfile-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/Update-VersionFile"
      }
    }
  ]
}
